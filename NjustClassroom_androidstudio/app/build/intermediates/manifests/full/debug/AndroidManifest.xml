<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ckj.njustclassroom"
    android:versionCode="16"
    android:versionName="2.9.1" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ckj.njustclassroom.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ckj.njustclassroom.SetActivity"
            android:label="@string/set" >
        </activity>
        <activity
            android:name="com.ckj.njustclassroom.ShowRoomsActivity"
            android:label="@string/room" >
        </activity>
        <activity android:name="com.ckj.njustclassroom.MyScheduleActivity" >
        </activity>
        <activity android:name="com.ckj.njustclassroom.MyGradeActivity" >
        </activity>
        <activity android:name="com.ckj.njustclassroom.MyClassroomActivity" >
        </activity>
        <activity android:name="com.ckj.njustclassroom.WebViewActivity" >
        </activity>
        <activity android:name="com.ckj.njustclassroom.WebViewActivity2" >
        </activity>
    </application>

</manifest>